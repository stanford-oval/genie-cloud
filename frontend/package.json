    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "name": "almond-frontend",
  "version": "0.0.1",
  "main": "server/index.js",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "NODE_ENV=production next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "start": "nodemon -w server server/index.js",
    "serve": "NODE_ENV=production node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 8.12"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "repository": "https://github.com/stanford-oval/almond-cloud",
  "bugs": "https://github.com/stanford-oval/almond-cloud/issues",
  "author": "Euirim Choi <euirim@stanford.edu> (https://github.com/euirim)",
  "license": "MIT",
  "description": "Frontend of Almond's website.",
  "private": true
}
