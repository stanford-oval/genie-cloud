{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "((^|\\/)_|\\/tests?\\/)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
          "useLongnameInNav": true
        }
    }
}
